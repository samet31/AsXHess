                    TextButton.TextColor3 = Color3.fromRGB(43, 246, 249)
                    TextButton.TextSize = 14.000

                    TextLabel.Parent = PageItemTest
                    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextLabel.BackgroundTransparency = 1.000
                    TextLabel.Position = UDim2.new(0.0371747203, 0, 0, 0)
                    TextLabel.Size = UDim2.new(0.724907041, 0, 1, 0)
                    TextLabel.ZIndex = 5
                    TextLabel.Font = Enum.Font.ArialBold
                    TextLabel.Text = name or "AstenyUI TextButton"
                    TextLabel.TextColor3 = Color3.fromRGB(211, 211, 211)
                    TextLabel.TextSize = 14.000
                    TextLabel.TextXAlignment = Enum.TextXAlignment.Left

                    touch_app.Name = "touch_app"
                    touch_app.Parent = PageItemTest
                    touch_app.BackgroundTransparency = 1.000
                    touch_app.Position = UDim2.new(0.889999986, 0, 0, 2)
                    touch_app.Size = UDim2.new(0, 25, 0, 25)
                    touch_app.ZIndex = 5

                end

                function pageitems:AddCheckBox(adis,default,callback)
                    local CheckBox = Instance.new("ImageLabel")
                    local TextButton = Instance.new("TextButton")
                    local TextLabel = Instance.new("TextLabel")
                    local radio_button_unchecked = Instance.new("ImageButton")

                    CheckBox.Name = "CheckBox"
                    CheckBox.Parent = Page
                    CheckBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    CheckBox.BackgroundTransparency = 1.000
                    CheckBox.Size = UDim2.new(0, 269, 0, 32)
                    CheckBox.ZIndex = 3
                    CheckBox.Image = "rbxassetid://3570695787"
                    CheckBox.ImageColor3 = Color3.fromRGB(27, 27, 27)
                    CheckBox.ScaleType = Enum.ScaleType.Slice
                    CheckBox.SliceCenter = Rect.new(100, 100, 100, 100)
                    CheckBox.SliceScale = 0.085

                    TextButton.Parent = CheckBox
                    TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextButton.BackgroundTransparency = 1.000
                    TextButton.Size = UDim2.new(0, 266, 0, 33)
                    TextButton.ZIndex = 4
                    TextButton.Font = Enum.Font.SourceSans
                    TextButton.Text = ""
                    TextButton.TextColor3 = Color3.fromRGB(43, 246, 249)
                    TextButton.TextSize = 14.000

                    TextLabel.Parent = CheckBox
                    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextLabel.BackgroundTransparency = 1.000
                    TextLabel.Position = UDim2.new(0.0371747203, 0, 0, 0)
                    TextLabel.Size = UDim2.new(0.724907041, 0, 1, 0)
                    TextLabel.ZIndex = 5
                    TextLabel.Font = Enum.Font.ArialBold
                    TextLabel.Text = adis or "Unnamed CheckBox"
                    TextLabel.TextColor3 = Color3.fromRGB(211, 211, 211)
                    TextLabel.TextSize = 14.000
                    TextLabel.TextXAlignment = Enum.TextXAlignment.Left

                    radio_button_unchecked.Name = "radio_button_unchecked"
                    radio_button_unchecked.Parent = CheckBox
                    radio_button_unchecked.BackgroundTransparency = 1.000
                    radio_button_unchecked.Position = UDim2.new(0.889999986, 0, 0, 3)
                    radio_button_unchecked.Size = UDim2.new(0, 25, 0, 25)
                    radio_button_unchecked.ZIndex = 11
                    radio_button_unchecked.Image = "rbxassetid://3926309567"
                    radio_button_unchecked.ImageColor3 = Color3.fromRGB(43, 255, 110)
                    radio_button_unchecked.ImageRectOffset = Vector2.new(628, 420)
                    radio_button_unchecked.ImageRectSize = Vector2.new(48, 48)

                    local delay = false
                    local checked = default
                    if checked == true then
                        radio_button_unchecked.ImageRectOffset = Vector2.new(784, 420)
                        pcall(callback,true)
                    else
                        radio_button_unchecked.ImageRectOffset = Vector2.new(628, 420)
                        pcall(callback,false)
                    end
                    CheckBox.MouseEnter:Connect(function()
                        CheckBox.ImageColor3 = Color3.fromRGB(55,55,55)
                    end)
                    CheckBox.MouseLeave:Connect(function()
                        CheckBox.ImageColor3 = Color3.fromRGB(27,27,27)
                    end)

                    TextButton.MouseButton1Click:Connect(function()
                    if delay == false then
                        CircleClick(CheckBox,Mouse.X,Mouse.Y)
                        if checked == true then
                                pcall(callback,false)
                                checked = false
                                radio_button_unchecked.ImageRectOffset = Vector2.new(628, 420)
                                delay = true
                                wait(0.2)
                                delay = false
                        else
                                pcall(callback,true)
                                checked = true
                                radio_button_unchecked.ImageRectOffset = Vector2.new(784, 420)
                                delay = true
                                wait(0.2)
                                delay = false
                        end
                    end
                    end)
                end

                function pageitems:AddListbox(isim,items,default,callback)


                    isim = isim or "Listbox"
                    items = items or {"1","2","3"}
                    default = default or ""
                    callback = callback or function () end
                        
                    local Listbox = Instance.new("ImageLabel")
                    local TextButton = Instance.new("TextButton")
                    local TextLabel = Instance.new("TextLabel")
                    local radio_button_unchecked = Instance.new("ImageButton")
                    local dropdownitems = Instance.new("ScrollingFrame")
                    local UIListLayout4 = Instance.new("UIListLayout")
                    
                    
                    
                    Listbox.Name = "Listbox"
                    Listbox.Parent = Page
                    Listbox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    Listbox.BackgroundTransparency = 1.000
                    Listbox.ClipsDescendants = true
                    Listbox.Position = UDim2.new(0, 0, 0.22265625, 0)
                    Listbox.Size = UDim2.new(0, 269, 0, 33)--167
                    Listbox.ZIndex = 3
                    Listbox.Image = "rbxassetid://3570695787"
                    Listbox.ImageColor3 = Color3.fromRGB(27, 27, 27)
                    Listbox.ScaleType = Enum.ScaleType.Slice
                    Listbox.SliceCenter = Rect.new(100, 100, 100, 100)
                    Listbox.SliceScale = 0.085
                    
                    TextButton.Parent = Listbox
                    TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextButton.BackgroundTransparency = 1.000
                    TextButton.Size = UDim2.new(0, 266, 0, 33)
                    TextButton.ZIndex = 4
                    TextButton.Font = Enum.Font.SourceSans
                    TextButton.Text = ""
                    TextButton.TextColor3 = Color3.fromRGB(43, 246, 249)
                    TextButton.TextSize = 14.000
                    
                    TextLabel.Parent = Listbox
                    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextLabel.BackgroundTransparency = 1.000
                    TextLabel.Position = UDim2.new(0.0371747203, 0, 0, 0)
                    TextLabel.Size = UDim2.new(0, 195, 0, 32)
                    TextLabel.ZIndex = 5
                    TextLabel.Font = Enum.Font.ArialBold
                    TextLabel.Text = isim
                    TextLabel.TextColor3 = Color3.fromRGB(211, 211, 211)
                    TextLabel.TextSize = 14.000
                    TextLabel.TextXAlignment = Enum.TextXAlignment.Left
                    
                    radio_button_unchecked.Name = "radio_button_unchecked"
                    radio_button_unchecked.Parent = Listbox
                    radio_button_unchecked.BackgroundTransparency = 1.000
                    radio_button_unchecked.Position = UDim2.new(0.889999986, 0, 0, 3)
                    radio_button_unchecked.Size = UDim2.new(0, 25, 0, 25)
                    radio_button_unchecked.ZIndex = 11
                    radio_button_unchecked.Image = "rbxassetid://3926305904"
                    radio_button_unchecked.ImageColor3 = Color3.fromRGB(43, 255, 110)
                    radio_button_unchecked.ImageRectOffset = Vector2.new(124, 924)
                    radio_button_unchecked.ImageRectSize = Vector2.new(36, 36)
                    
                    dropdownitems.Name = "dropdownitems"
                    dropdownitems.Parent = Listbox
                    dropdownitems.Active = true
                    dropdownitems.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
                    dropdownitems.BackgroundTransparency = 1
                    dropdownitems.BorderSizePixel = 0
                    dropdownitems.Position = UDim2.new(0, 12, 0, 38)
                    dropdownitems.Size = UDim2.new(0, 243, 0, 112)
                    dropdownitems.ZIndex = 31
                    dropdownitems.ScrollBarThickness = 3

                    

                    local items3 =  {}

                    local Entry = Instance.new("ImageLabel")


                    Entry.Name = "Entry"
                    Entry.Parent = Listbox
                    Entry.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    Entry.BackgroundTransparency = 1.000
                    Entry.Size = UDim2.new(0, 269, 0, 32)
                    Entry.ZIndex = 3
                    Entry.Image = "rbxassetid://3570695787"
                    Entry.ImageColor3 = Color3.fromRGB(27, 27, 27)
                    Entry.ScaleType = Enum.ScaleType.Slice
                    Entry.SliceCenter = Rect.new(100, 100, 100, 100)
                    Entry.SliceScale = 0.085
                    
                    UIListLayout4.Parent = dropdownitems
                    UIListLayout4.SortOrder = Enum.SortOrder.LayoutOrder

                    local selecteditem = default;
                    dropdownitems.ChildAdded:Connect(function(chld)
                        wait(.01)
                        dropdownitems.CanvasSize = UDim2.new(0,0,0,UIListLayout4.AbsoluteContentSize.Y);
                    end)

                    for i,v in pairs(items) do
                        local TextButton = Instance.new("TextButton")
                        local TextButton_Roundify_8px = Instance.new("ImageLabel")


                        TextButton.Parent = dropdownitems
                        TextButton.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
                        TextButton.BackgroundTransparency = 1.000
                        TextButton.BorderSizePixel = 0
                        TextButton.Size = UDim2.new(0, 241, 0, 28)
                        TextButton.ZIndex = 32
                        TextButton.Font = Enum.Font.ArialBold
                        TextButton.Text = v
                        TextButton.TextColor3 = Color3.fromRGB(133,133,133)

                        TextButton.Name = v
                        TextButton.TextSize = 14.000

                        TextButton_Roundify_8px.Name = "TextButton_Roundify_8px"
                        TextButton_Roundify_8px.Parent = TextButton
                        TextButton_Roundify_8px.Active = true
                        TextButton_Roundify_8px.AnchorPoint = Vector2.new(0.5, 0.5)
                        TextButton_Roundify_8px.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TextButton_Roundify_8px.BackgroundTransparency = 1.000
                        TextButton_Roundify_8px.Position = UDim2.new(0.5, 0, 0.5, 0)
                        TextButton_Roundify_8px.Selectable = true
                        TextButton_Roundify_8px.Size = UDim2.new(1, 0, 1, 0)
                        TextButton_Roundify_8px.ZIndex = 31
                        TextButton_Roundify_8px.Image = "rbxassetid://3570695787"
                        TextButton_Roundify_8px.ImageColor3 = Color3.fromRGB(27, 27, 27)
                        TextButton_Roundify_8px.ScaleType = Enum.ScaleType.Slice
                        TextButton_Roundify_8px.SliceCenter = Rect.new(100, 100, 100, 100)
                        TextButton_Roundify_8px.SliceScale = 0.080

                        if v == selecteditem then
                            TextButton.TextColor3 = Color3.fromRGB(211,211,211)
                            pcall(callback,selecteditem)
                            TextLabel.Text = selecteditem
                        end

                        TextButton.MouseEnter:Connect(function()
                            TextButton_Roundify_8px.ImageColor3 = Color3.fromRGB(55,55,55)
                            TextButton.TextColor3 = Color3.fromRGB(211,211,211)
                        end)
                        TextButton.MouseLeave:Connect(function()
                            TextButton_Roundify_8px.ImageColor3 = Color3.fromRGB(27, 27, 27)
                            if v ~= selecteditem then
                                TextButton.TextColor3 = Color3.fromRGB(133,133,133)
                            end 
                        end)
                        TextButton.MouseButton1Click:Connect(function()
                            for i2,v2 in pairs(dropdownitems:GetChildren()) do
                                if v2:IsA("TextButton") then
                                    v2.TextColor3 = Color3.fromRGB(133,133,133)
                                end
                            end
                            selecteditem = v
                            TextLabel.Text = v
                            TextButton.TextColor3 = Color3.fromRGB(211,211,211)
                            pcall(callback,selecteditem)
                        end)
                    end
                    function  items3:Resfresh(neblm)
                        for i , v in pairs(dropdownitems:GetChildren()) do
                            if v:IsA("TextButton") then
                                v:Destroy()
                            end
                        end
                        for i,v in pairs(neblm) do
                            local TextButton = Instance.new("TextButton")
                            local TextButton_Roundify_8px = Instance.new("ImageLabel")
    
    
                            TextButton.Parent = dropdownitems
                            TextButton.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
                            TextButton.BackgroundTransparency = 1.000
                            TextButton.BorderSizePixel = 0
                            TextButton.Size = UDim2.new(0, 241, 0, 28)
                            TextButton.ZIndex = 32
                            TextButton.Font = Enum.Font.ArialBold
                            TextButton.Text = v
                            TextButton.TextColor3 = Color3.fromRGB(133,133,133)
    
                            TextButton.Name = v
                            TextButton.TextSize = 14.000
    
                            TextButton_Roundify_8px.Name = "TextButton_Roundify_8px"
                            TextButton_Roundify_8px.Parent = TextButton
                            TextButton_Roundify_8px.Active = true
                            TextButton_Roundify_8px.AnchorPoint = Vector2.new(0.5, 0.5)
                            TextButton_Roundify_8px.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                            TextButton_Roundify_8px.BackgroundTransparency = 1.000
                            TextButton_Roundify_8px.Position = UDim2.new(0.5, 0, 0.5, 0)
                            TextButton_Roundify_8px.Selectable = true
                            TextButton_Roundify_8px.Size = UDim2.new(1, 0, 1, 0)
                            TextButton_Roundify_8px.ZIndex = 31
                            TextButton_Roundify_8px.Image = "rbxassetid://3570695787"
                            TextButton_Roundify_8px.ImageColor3 = Color3.fromRGB(27, 27, 27)
                            TextButton_Roundify_8px.ScaleType = Enum.ScaleType.Slice
                            TextButton_Roundify_8px.SliceCenter = Rect.new(100, 100, 100, 100)
                            TextButton_Roundify_8px.SliceScale = 0.080
    
                            if v == selecteditem then
                                TextButton.TextColor3 = Color3.fromRGB(211,211,211)
                                pcall(callback,selecteditem)
                                TextLabel.Text = selecteditem
                            end
    
                            TextButton.MouseEnter:Connect(function()
                                TextButton_Roundify_8px.ImageColor3 = Color3.fromRGB(55,55,55)
                                TextButton.TextColor3 = Color3.fromRGB(211,211,211)
                            end)
                            TextButton.MouseLeave:Connect(function()
                                TextButton_Roundify_8px.ImageColor3 = Color3.fromRGB(27, 27, 27)
                                if v ~= selecteditem then
                                    TextButton.TextColor3 = Color3.fromRGB(133,133,133)
                                end 
                            end)
                            TextButton.MouseButton1Click:Connect(function()
                                for i2,v2 in pairs(dropdownitems:GetChildren()) do
                                    if v2:IsA("TextButton") then
                                        v2.TextColor3 = Color3.fromRGB(133,133,133)
                                    end
                                end
                                selecteditem = v
                                TextLabel.Text = v
                                TextButton.TextColor3 = Color3.fromRGB(211,211,211)
                                pcall(callback,selecteditem)
                            end)
                        end

                    end
                   

                
                    Entry.MouseEnter:Connect(function()
                        Entry.ImageColor3 = Color3.fromRGB(55,55,55)
                    end)
                    Entry.MouseLeave:Connect(function()
                        Entry.ImageColor3 = Color3.fromRGB(27,27,27)
                    end)
                    local cd = false
                    local dropped = false
                    TextButton.MouseButton1Click:Connect(function()
                       if cd == false then
                        if dropped == false then
                           CircleClick(Entry,Mouse.X,Mouse.Y)
                           TS:Create(radio_button_unchecked,TweenInfo.new(0.5),{Rotation = -90}):Play()
                           TS:Create(Listbox,TweenInfo.new(0.25),{Size = UDim2.new(0, 269, 0, 167)}):Play()
                           TS:Create(Page,TweenInfo.new(0.25),{CanvasSize = UDim2.new(0,0,0,UIListLayout_2.AbsoluteContentSize.Y + 134)}):Play()
                           cd = true
                           wait(1)
                           dropped = true
                           cd = false
                           else
                            CircleClick(Entry,Mouse.X,Mouse.Y)
                            TS:Create(radio_button_unchecked,TweenInfo.new(0.5),{Rotation = 0}):Play()
                            TS:Create(Listbox,TweenInfo.new(0.5),{Size = UDim2.new(0, 269, 0, 33)}):Play()
                            TS:Create(Page,TweenInfo.new(0.5),{CanvasSize = UDim2.new(0,0,0,UIListLayout_2.AbsoluteContentSize.Y - 134)}):Play()
                            cd = true
                            wait(1)
                            dropped = false
                            cd = false
                        end
                       end
                    end)
                    return items3
                end
                function pageitems:addKeybind(name,default,callback)
                    local Keybind = Instance.new("ImageLabel")
                    local isim = Instance.new("TextLabel")
                    local CheckBox = Instance.new("ImageLabel")
                    local bind = Instance.new("TextLabel")
                    local TextButton = Instance.new("TextButton")



                    Keybind.Name = "Keybind"
                    Keybind.Parent = Page
                    Keybind.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    Keybind.BackgroundTransparency = 1.000
                    Keybind.Size = UDim2.new(0, 269, 0, 32)
                    Keybind.ZIndex = 3
                    Keybind.Image = "rbxassetid://3570695787"
                    Keybind.ImageColor3 = Color3.fromRGB(27, 27, 27)
                    Keybind.ScaleType = Enum.ScaleType.Slice
                    Keybind.SliceCenter = Rect.new(100, 100, 100, 100)
                    Keybind.SliceScale = 0.085

                    isim.Name = "isim"
                    isim.Parent = Keybind
                    isim.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    isim.BackgroundTransparency = 1.000
                    isim.Position = UDim2.new(0.0371747203, 0, 0, 0)
                    isim.Size = UDim2.new(0.724907041, 0, 1, 0)
                    isim.ZIndex = 5
                    isim.Font = Enum.Font.ArialBold
                    isim.Text = name
                    isim.TextColor3 = Color3.fromRGB(211, 211, 211)
                    isim.TextSize = 14.000
                    isim.TextXAlignment = Enum.TextXAlignment.Left

                    CheckBox.Name = "CheckBox"
                    CheckBox.Parent = Keybind
                    CheckBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    CheckBox.BackgroundTransparency = 1.000
                    CheckBox.Position = UDim2.new(0.717999995, 0, 0.063000001, 3)
                    CheckBox.Size = UDim2.new(0, 68, 0, 23)
                    CheckBox.ZIndex = 4
                    CheckBox.Image = "rbxassetid://3570695787"
                    CheckBox.ImageColor3 = Color3.fromRGB(15, 15, 15)
                    CheckBox.ScaleType = Enum.ScaleType.Slice
                    CheckBox.SliceCenter = Rect.new(100, 100, 100, 100)
                    CheckBox.SliceScale = 0.085

                    bind.Name = "bind"
                    bind.Parent = CheckBox
                    bind.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    bind.BackgroundTransparency = 1.000
                    bind.Size = UDim2.new(1, 0, 1, 0)
                    bind.ZIndex = 5
                    bind.Font = Enum.Font.Arial
                    bind.Text = default or ". . ."
                    bind.TextColor3 = Color3.fromRGB(211, 211, 211)
                    bind.TextSize = 14.000

                    TextButton.Parent = Keybind
                    TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    TextButton.BackgroundTransparency = 1.000
                    TextButton.Size = UDim2.new(1, 0, 1, 0)
                    TextButton.ZIndex = 6
                    TextButton.Font = Enum.Font.SourceSans
                    TextButton.Text = ""
                    TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                    TextButton.TextSize = 14.000

                    local cd = false
                    local keybind = default
                    local changebind = false
                    pcall(callback,keybind)
                    TextButton.MouseButton1Click:Connect(function()
                        if cd == false then
                            wait(.1)
                            cd = true
                            bind.Text = ". . ."
                            changebind = true
                        end
                        UIS.InputBegan:Connect(function(key)
                            if changebind then
                                changebind = false
                                keybind = key.KeyCode.Name
                                bind.Text = keybind
                                wait(.1)
                                pcall(callback,keybind)
                                cd = false
                            end
                        end)
                    end)   
                end
                
            return pageitems

        end
        return items
        
end

return AstenyUI

                    deger.TextWrapped = true
                    deger.TextXAlignment = Enum.TextXAlignment.Right

                    local minvalue = mindeger or 0
                    local maxvalue = maxdeger or 100


                    callback = callback or function() end


                    local mouse = game.Players.LocalPlayer:GetMouse()
                    local uis = game:GetService("UserInputService")
                    local Value = baslangicdeger


                    local a = maxdeger - mindeger
                    local b = baslangicdeger - mindeger
                    local c = b/a
                    local d = c * sldrbtn.AbsoluteSize.X
                    sldrrenk.Size  = UDim2.new(0,d,1,0)
                    deger.Text = Value
                    sldrbtn.MouseButton1Down:Connect(function()
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 255) * sldrrenk.AbsoluteSize.X) + tonumber(minvalue)) or 0
                        pcall(function()
                            callback(Value)
                        end)
                        sldrrenk.Size = UDim2.new(0, math.clamp(mouse.X - sldrrenk.AbsolutePosition.X, 0, 255), 0, 11)
                        moveconnection = mouse.Move:Connect(function()
                            deger.Text = Value
                            Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 255) * sldrrenk.AbsoluteSize.X) + tonumber(minvalue))
                            pcall(function()
                                callback(Value)
                            end)
                            sldrrenk.Size = UDim2.new(0, math.clamp(mouse.X - sldrrenk.AbsolutePosition.X, 0, 255), 0, 11)
                        end)
                        releaseconnection = uis.InputEnded:Connect(function(Moouse)
                            if Moouse.UserInputType == Enum.UserInputType.MouseButton1 then
                                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 255) * sldrrenk.AbsoluteSize.X) + tonumber(minvalue))
                                pcall(function()
                                    callback(Value)
                                end)
                                deger.Text = Value
                                sldrrenk.Size = UDim2.new(0, math.clamp(mouse.X - sldrrenk.AbsolutePosition.X, 0, 255), 0, 11)
                                moveconnection:Disconnect()
                                releaseconnection:Disconnect()
                            end
                        end)
                    end)
                end

                function pageitems:AddBtn(name,callback)

                    

                    local PageItemTest = Instance.new("ImageLabel")
                    local TextButton = Instance.new("TextButton")
                    local TextLabel = Instance.new("TextLabel")
                    local touch_app = Instance.new("ImageButton")


                    PageItemTest.Name = "TextBtn"
                    PageItemTest.Parent = Page
